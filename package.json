{
  "name": "klickpark-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A webpack boilerplate for static websites that has all the necessary modern tools and optimizations built-in.",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "net": "webpack-dev-server --open --port 8888 --hot --host 0.0.0.0",
    "build": "webpack --config webpack.prod.js --progress",
    "deploy": "rsync -rz --progress --delete ./dist/* user@host:/path/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Klickpark/klickpark-webpack-boilerplate"
  },
  "keywords": [],
  "author": "Klickpark",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "offline-plugin": "^5.0.6",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-url": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "lazysizes": "^4.1.5"
  }
}
